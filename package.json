{
  "name": "@arcia125/custom-dropdown",
  "private": false,
  "version": "0.5.2",
  "description": "Custom dropdown Web Component",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/custom-dropdown/custom-dropdown.esm.js",
  "exports": {
    ".": {
      "import": "./dist/custom-dropdown/custom-dropdown.esm.js",
      "require": "./dist/custom-dropdown/custom-dropdown.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Arcia125/custom-dropdown-stencil.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "deploy-demo": "gh-pages -d www"
  },
  "devDependencies": {
    "@stencil/core": "^4.7.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "gh-pages": "^6.2.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0"
  },
  "license": "MIT",
  "author": {
    "name": "Kevin Hallett",
    "email": "kevinmhallett@gmail.com",
    "url": "https://www.kevinmhallett.com"
  }
}
